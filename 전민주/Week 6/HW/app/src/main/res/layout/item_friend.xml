<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/item_friend_image"
        android:layout_width="87dp"
        android:layout_height="74dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
         />

    <TextView
        android:id="@+id/item_friend_tvId"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="12dp"
        android:text="asdf"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toEndOf="@id/item_friend_image"
         />

    <TextView
        android:id="@+id/item_friend_tvSelfInfo"
        android:layout_width="150dp"
        android:layout_height="70dp"
        android:layout_marginStart="160dp"
        android:layout_marginBottom="4dp"
        android:text="asdfasdf"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@id/item_friend_tvId" />

    <TextView
        android:id="@+id/item_friend_tvBirthDay"
        android:layout_width="81dp"
        android:layout_height="59dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="40dp"
        android:text="오늘 생일"
        android:visibility="gone"
        app:layout_constraintStart_toEndOf="@+id/item_friend_tvId"
        app:layout_constraintTop_toTopOf="@+id/item_friend_tvSelfInfo" />


</androidx.constraintlayout.widget.ConstraintLayout>